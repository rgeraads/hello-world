{
    "name": "rgeraads/hello-world",
    "description": "Hello world",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/flex": "^1.7",
        "symfony/framework-bundle": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
